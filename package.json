{
  "name": "drivingschool",
  "version": "1.0.0",
  "description": "个人的第一个全栈项目",
  "main": "index.js",
  "scripts": {
    "start": "set NODE_ENV=base && webpack --progress --colors",
    "server": "set NODE_ENV=base && webpack-dev-server --hot --colors",
    "watch": "set NODE_ENV=base && webpack --watch --colors --display-error-details --verbose",
    "dev": "set NODE_ENV=dev && webpack-dev-server --open",
    "build": "set NODE_ENV=prod && webpack -p",
    "lint": "set NODE_ENV=lint && webpack-dev-server --open",
    "serve": "http-server ./public -p 8888 -o",
    "serve2": "http-server ./public -p 8888"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tomotoes/drivingSchool.git"
  },
  "keywords": [
    "全栈"
  ],
  "author": "Simon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tomotoes/drivingSchool/issues"
  },
  "homepage": "https://github.com/Tomotoes/drivingSchool#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "ejs-loader": "^0.3.0",
    "expose-loader": "^0.7.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "image-webpack-loader": "^3.4.2",
    "jquery": "^3.2.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "open-browser-webpack-plugin": "0.0.5",
    "post-loader": "^2.0.0",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "url-loader": "^0.6.2",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  }
}
